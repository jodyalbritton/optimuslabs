 

<div class="panel" id="client-invoices">
          <div class="panel-heading">
            <span class="panel-title">Client Invoices</span>
          </div>
          <div class="panel-body">
            <div>
            <table class="table table-striped">
              <thead>
                <tr>
                  <th>#</th>
                  <th>Subject</th>
                </tr>
              </thead>
              <tbody>
              <% @client.invoices.each do |invoice|%>
                <tr>
                  <td><%= invoice.id %></td>
                  <td><%= invoice.subject %></td>
                </tr>
              <% end %>
              </tbody>
            </table>
            </div>
          </div>
        </div>
