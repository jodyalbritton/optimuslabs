<% if defined? result %>
	<% if result == "positive" %>
	<div class="alert alert-success  alert-dismissible" id="vote-<%=votable.id %>-alert" data-dismiss="alert">
		<button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
		+1
	</div>
	<% else %>
	<div class="alert alert-danger  alert-dismissible" id="vote-<%=votable.id%>-alert" data-dismiss="alert">
		<button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
		-1
	</div>


	<% end %>
<% end %>